{
    "language"        : "PYTHON",
    "main-file"       : "run_spearmint.py",
    "experiment-name" : "none",
    "likelihood"      : "GAUSSIAN",
    "resources"       : {
        "my-machine" : {
            "scheduler" : "local",
            "max-concurrent" : 1,
            "max-finished-jobs" : 1 
        }
    },
    "variables" : {
"batch_size" : { "min" : 100, "max" : 512, "type" : "int", "size" : 1 },
"cost_type" : { "options" : ["xent", "h1", "h2"], "type" : "enum", "size" : 1 },
"default_input_include_prob" : { "min" : 1.0e-1, "max" : 1, "type" : "float", "size" : 1 },
"default_input_scale" : { "min" : 1.0e-1, "max" : 5, "type" : "float", "size" : 1 },
"fc_dim0" : { "min" : 57, "max" : 1000, "type" : "int", "size" : 1 },
"fc_dim1" : { "min" : 57, "max" : 500, "type" : "int", "size" : 1 },
"fc_dim2" : { "min" : 57, "max" : 250, "type" : "int", "size" : 1 },
"fc_layer_type" : { "options" : ["RectifiedLinear", "Tanh", "Sigmoid"], "type" : "enum", "size" : 1 },
"input_dropout" : { "min" : 1.0e-1, "max" : 1, "type" : "float", "size" : 1 },
"input_scale" : { "min" : 1.0e-1, "max" : 5, "type" : "float", "size" : 1 },
"log_decay_eps" : { "min" : -5, "max" : -1, "type" : "float", "size" : 1 },
